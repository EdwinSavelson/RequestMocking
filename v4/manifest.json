{
    "manifest_version": 3,
    "name": "Extension Name",
    "description": "Some Desc.",
    "version": "1.1",
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "run_at": "document_start",
        "js": ["contentscript.js", "inject.js"]
    }],
    "web_accessible_resources": [{
        "resources": ["injected.js"],
        "matches": ["<all_urls>"]
    }]
  }